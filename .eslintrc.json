{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "plugins": [
    "react",
    "jsx-a11y",
    "import",
    "mocha"
  ],
  "rules": {
    "comma-dangle": ["error", "never"],
    "react/jsx-curly-spacing": [2, "always"],
    "mocha/no-exclusive-tests": "error"
  },
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  }
}